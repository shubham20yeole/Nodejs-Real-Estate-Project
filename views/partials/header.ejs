    <![endif]-->

  </head>
  <body class="aa-price-range">  
  <!-- Pre Loader -->
  <div id="aa-preloader-area">
    <div class="pulse"></div>
  </div>
  <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
  <!-- END SCROLL TOP BUTTON -->

  <!-- Start header section -->
  <header id="aa-header">  
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          <div class="aa-header-area">
            <div class="row">
              <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="aa-header-left">
                  <div class="aa-telephone-no">
                    <span class="fa fa-phone"></span>
                     1-201-887-5323
                  </div>
                  <div class="aa-email hidden-xs">
                    <span class="fa fa-envelope-o"></span> shubham20.yeole@gmail.com
                  </div>
                </div>              
              </div>
              <div class="col-md-6 col-sm-6 col-xs-6">
                <div class="aa-header-right">
                   <% if(users.fullname == 'Anonymous'){%>
                    <a href="registerlogin" class="aa-register">SIGN UP</a>
                    <a href="registerlogin" class="aa-login">SIGN IN</a> 
                  <%}else{%> 
                      <img src="<%=users.photo%>" width="32" height="32"> 
                      <a href="/logout" class="aa-login"><%= users.fullname %></a>
                      | <a href="/logout" class="aa-login">LOGOUT</a>
                      | <a href="/profile/0" class="aa-login">PROFILE</a>
                  <%}%>
                   <% if(users.type == 'admin'){%>
                    <a href="../admin" class="aa-register"> | ADMIN</a>
                  <%}%>            
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </header>
  <!-- End header section -->

  <!-- Start menu section -->
  <section id="aa-menu-area">
    <nav class="navbar navbar-default main-navbar" role="navigation">  
      <div class="container">
        <div class="navbar-header">
          <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <!-- LOGO -->                                               
          <!-- Text based logo -->
           <a class="navbar-brand aa-logo" href="/"> SHUBHAM <span>GREAT LIVINGS</span></a>
           
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
            <li class="active"><a href="/">HOME</a></li>
             
             <li class="dropdown active">
              <a class="dropdown-toggle" data-toggle="dropdown">RENT <span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">                
                <li><a href="/properties/0">ALL RENTAL LISTINGS</a></li>
                <li><a href="/gallery/0">ALL RENTAL IMAGES</a></li>                                
              </ul>
           </li>
             <li class="dropdown active">
              <a class="dropdown-toggle" data-toggle="dropdown">SELL<span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                <li><a href="/postadd">POST YOUR PROPERTY</a></li>                
              </ul>
           </li>
           <li class="dropdown active">
              <a class="dropdown-toggle" data-toggle="dropdown">MAP<span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                  <li><a href="../propertiesbymaps">MAP + NYC-PROPERTY</a></li>                     
                  <li><a href="../propertiesbymaps2">MAP + USA-PROPERTY</a></li>                    
              </ul>
           </li>
          <li class="active"><a href="/contact">CONTACT</a></li>
          <% if(users.fullname == 'Anonymous'){%>
            <li class="active"><a href="registerlogin" class="aa-register">SIGN UP</a></li>
             <li class="active"><a href="registerlogin" class="aa-login">SIGN IN</a></li>
          <%}else{%> 
           <li class="dropdown active">
              <a class="dropdown-toggle" data-toggle="dropdown"><img src="<%=users.photo%>" class="clicleimg" width="16" height="16"> &nbsp;&nbsp;Profile<span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                 <li><a href="/profile/0" class="aa-login" target="_blank">PROFILE</a></li>
                   <% if(users.type == 'admin'){%>
                  <li><a href="../admin" class="aa-register" target="_blank">ADMIN</a></li>
                  <%}%>                     
                 <li><a href="/logout" class="aa-login">LOGOUT</a></li>
              </ul>
           </li>
           <li id="notification_li">
              <a href="#" id="notificationLink"><span id="notification_count"><%= notifications.length %></span></a>
              <div id="notificationContainer">
              <div id="notificationTitle">Notifications</div>
              <div id="notificationsBody" class="notifications" style="overflow:scroll; height:400px;">
              <table width="100%" class="notificationstable">
              <% notifications.forEach(function(notifications){%>
                <tr>
                  <td><img src="<%= notifications.noteimage %>" width="70" height="70"></td>
                  <td>
                    <span class="heading"><%= notifications.action %></span><br>
                    <%= notifications.dateField %><br><%= notifications.message %></td>
                  </tr>
                <tr>
                  <td><hr></td>
                  <td>
                    <hr></td>
                </tr>
                <%}) %>
              </table>   
              </div>
              <div id="notificationFooter"><a href="/profile/0" target="_blank">See All</a></div>
              </div>
            </li>
          <%}%>          
          </ul>                         
        </div>

      </div>          
    </nav> 
  </section>
       